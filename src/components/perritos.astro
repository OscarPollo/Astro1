---

const res = await fetch("https://dogapi.dog/api/v2/breeds");
const data = await res.json();

console.log(data);


---

<div class="grid gap-8 sn:grid-cols-2 lg:grid-cols-3">
    <article>
        <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
            <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark">
                <tr>
                    <th>Nombre</th>
                    <th>Descripcion</th>
                    <th>Hipoalargenico</th>
                </tr>
            </thead>
            <tbody>
                {data.data.map((breed: any) => (
                    <tr>
                        <td>{breed.attributes.name || "N/A"}</td>
                        <td>{breed.attributes.description || "N/A"}</td>
                        <td>{breed.attributes.hypoallergenic.toString() || "N/A"}</td>
                    </tr>
                ))}
            </tbody>
        </table>
    </article>
</div>

